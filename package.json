{
  "name": "garlicoin",
  "description": "Garlicoin's main website",
  "version": "2.0.0",
  "author": "https://github.com/ronva",
  "scripts": {
    "build": "gatsby build",
    "deploy": "npm run update-release && gatsby build --prefix-paths && gh-pages -d public --repo https://github.com/GarlicoinOrg/garlicoin-web-prod.git",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-release": "node automation/fetchRelease.js"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.0.82",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.20",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-react-svg": "^2.0.0-beta1",
    "gatsby-plugin-root-import": "^2.0.4",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-zopfli": "^1.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2"
  },
  "keywords": [
    "cryptocurrency",
    "blockchain",
    "garlicoin",
    "gatsby"
  ]
}
